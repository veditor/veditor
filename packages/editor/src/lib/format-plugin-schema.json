{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Command format plugin",
  "type": "array",
  "items": {
    "type": "object",
    "required": [
      "key",
      "command"
    ],
    "properties": {
      "key": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "icon": {
        "type": "string"
      },
      "iconContent": {
        "type": "string"
      },
      "command": {
        "type": "string"
      },
      "commandArg": {
        "type": "string"
      },
      "hotKey": {
        "type": "string"
      }
    }
  }
}
